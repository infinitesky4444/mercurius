{
  "name": "recal",
  "version": "1.0.0b-7",
  "description": "A minimal, accessible React calendar component using modern CSS.",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "umd/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "BABEL_ENV=cjs babel src/lib -d cjs --copy-files",
    "build:umd": "BABEL_ENV=umd babel src/lib -d umd --copy-files",
    "build:esm": "BABEL_ENV=esm babel src/lib -d esm --copy-files",
    "lib:watch": "babel src/lib -w -d esm --copy-files",
    "docs": "webpack-dev-server",
    "docs:prod": "webpack -p"
  },
  "keywords": [
    "react",
    "calendar",
    "datepicker"
  ],
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.29.0"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.37",
    "@babel/core": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-react": "^7.0.0-beta.37",
    "autoprefixer": "^8.2.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "css-loader": "^0.28.8",
    "html-webpack-plugin": "^2.30.1",
    "postcss-loader": "^2.1.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "author": "Ruben Martinez Jr. <ruben.martinez93@gmail.com>",
  "homepage": "https://github.com/rubencodes/recal",
  "repository": {
    "type": "git",
    "url": "git@github.com:rubencodes/recal.git"
  }
}
